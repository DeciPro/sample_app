<p id="notice"><%= notice %></p>

<h1>Meetings</h1>

<table>
  <thead>
    <tr>
      <th>Meeting Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.name %></td>
        <td><%= link_to 'Show', user_meeting_path(meeting.user, meeting) %></td>
        <td><%= link_to 'Edit', edit_user_meeting_path(meeting.user, meeting) %></td>
        <td><%= link_to 'Destroy', user_meeting_path(meeting.user, meeting), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create Meetings', new_user_meeting_path %>
